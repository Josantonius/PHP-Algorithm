{
    "name":        "josantonius/algorithm",
    "version":     "1.1.4",
    "type":        "library",
    "description": "Class with algorithms to solve and display mathematical sequences.",
    "keywords": [
        "Algorithms",
        "Mathematical sequences",
        "Look-and-Say",
        "Conway constant",
        "HHVM",
        "PHP"
    ],
    "license": "MIT",
    "authors": [
        {
            "name":     "Josantonius",
            "email":    "hello@josantonius.com",
            "homepage": "https://josantonius.com",
            "role":     "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/josantonius/php-algorithm/issues",
        "forum":  "http://stackoverflow.com/tags/josantonius/php-algorithm",
        "source": "https://github.com/josantonius/php-algorithm"
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^5.6 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "Josantonius\\Algorithm\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Josantonius\\Algorithm\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
